(ns global/gene
  (class Object
  )

  (class File
    (fn read file
      (gene_file_read file)
    )

    (fn write [file content]
      (gene_file_write file content)
    )
  )

  (class String
    (method length _
      ($invoke self 'length')
    )
  )

  (fn Env name
    (gene_env_get name)
  )

  (fn Env/set [name value]
    (gene_env_set name value)
  )
)

