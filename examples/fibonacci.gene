#!/usr/bin/env grun

#< Use below command to compare performance against fibonacci in Ruby
time examples/fibonacci.gene ; time examples/fibonacci.rb 20
>#

(fn fibonacci n
  (if (n < 2)
    n
  else
    ((fibonacci (n - 1)) + (fibonacci (n - 2)))
  )
)

(print "fibonacci.gene: " (fibonacci 20) "\n")
