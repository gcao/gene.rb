#!/usr/bin/env grun

#< Use below command to compare performance against fibonacci in Ruby
examples/fibonacci.gene ; examples/fibonacci.rb 20
>#

(fn fibonacci n
  (if (n < 2)
    n
  else
    ((fibonacci (n - 1)) + (fibonacci (n - 2)))
  )
)

(var before (gene_invoke (gene_get_class "Time") "now"))
(print "fibonacci.gene: " (fibonacci 20) "\n")
(print "Used time: "
  ((gene_invoke (gene_get_class "Time") "now") - before)
  "\n"
)
